{
  "name": "form-bird",
  "version": "0.0.0",
  "main": "index.js",
  "repository": "https://github.com/senlima4/form-bird.git",
  "author": "Senlima Sun <senlima0430@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "vercel-build": "yarn prisma:generate && yarn prisma:migrate && next build",
    "prisma:generate": "PRISMA_CLIENT_ENGINE_TYPE='dataproxy' prisma generate",
    "prisma:migrate": "prisma migrate deploy"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^3.9.2",
    "apollo-server-micro": "^3.6.3",
    "argon2": "^0.28.4",
    "dotenv": "^16.0.0",
    "graphql": "^16.3.0",
    "micro-cors": "^0.1.1",
    "next": "^12.0.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/graphql": "^14.5.0",
    "@types/node": "^17.0.18",
    "@types/react": "^17.0.39",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "prisma": "^3.9.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 80,
    "endOfLine": "lf",
    "singleQuote": true,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "trailingComma": "es5",
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false
  }
}
